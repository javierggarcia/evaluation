{% block content %}
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    {% dump(fixture) %}
                    {% for team in fixture.teams %}
                        {{ team.name }}:{{ team.result }}
                    {% endfor %}
                </div>
                <div class="panel-body">
                   <p> Location: {{ fixture.location }} </p>
                   <p> Kickoff: {{ fixture.kickoff }}</p>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
